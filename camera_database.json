  "tutorials": [
    {
      "id": "tutorial_fx3_fx30_fx2",
      "type": "templated",
      "cameraIds": [
        "sony_fx3",
        "sony_fx30",
        "sony_fx2"
      ],
      "timeOfDaySteps": [
        {
          "id": "content_2",
          "type": "body",
          "text": "Press **[MENU]** > **[Shooting]** > **[7 TC/UB]**"
        },
        {
          "id": "content_3",
          "type": "list",
          "items": [
            "**Time Code Run** – Free Run",
            "**Time Code Preset** – Set this to the current time of day (second accurate is sufficient)"
          ]
        },
        {
          "id": "content_3b",
          "type": "current_timecode"
        },
        {
          "id": "content_3c",
          "type": "list",
          "items": [
            "**Time Code Make** – Preset",
            "**User Bit Time Rec** – Off"
          ]
        },
        {
          "id": "content_4",
          "type": "body",
          "text": "Press **[MENU]** > **[Setup]** > **[7 Display Option]**"
        },
        {
          "id": "content_5",
          "type": "list",
          "items": [
            "Set **TC/UB Disp. Setting** to **TC**"
          ]
        }
      ]
    },
