  "tutorials": [
    {
      "id": "sony_fx3_timecode",
      "cameraIds": ["sony_fx3"],
      "title": "Timecode Setup für Sony FX3",
      "steps": [
        {
          "id": "step1",
          "type": "text",
          "content": "Drücke die MENU Taste auf der Rückseite deiner Kamera."
        },
        {
          "id": "step2",
          "type": "imageWithText",
          "content": "Navigiere im Menü zu 'Recording' und dann zu 'TC/UB'.",
          "imageUrl": "https://raw.githubusercontent.com/ichbinjona/marker-camera-tutorials/main/assets/tutorials/sony_fx_menu_tcub.png",
          "caption": "Das TC/UB Menü unter Recording"
        },
        {
          "id": "step3",
          "type": "text",
          "content": "Wähle 'Timecode Preset' um die Startzeit einzustellen."
        },
        {
          "id": "step4",
          "type": "text",
          "content": "Setze 'TC Run' auf 'Rec Run' damit der Timecode nur während der Aufnahme läuft."
        },
        {
          "id": "step5",
          "type": "text",
          "content": "Fertig! Deine Sony FX3 zeichnet nun Timecode auf."
        }
      ]
    },
    {
      "id": "sony_fx30_fx6_timecode",
      "cameraIds": ["sony_fx30", "sony_fx6"],
      "title": "Timecode Setup",
      "steps": [
        {
          "id": "step1",
          "type": "text",
          "content": "Drücke die MENU Taste auf der Rückseite deiner Kamera."
        },
        {
          "id": "step2",
          "type": "text",
          "content": "Navigiere zu 'TC/UB' im Menü unter dem Abschnitt 'Recording'."
        },
        {
          "id": "step3",
          "type": "text",
          "content": "Wähle 'Timecode Preset' und stelle die gewünschte Startzeit ein."
        },
        {
          "id": "step4",
          "type": "text",
          "content": "Setze 'TC Run' auf 'Rec Run' für Timecode während der Aufnahme."
        },
        {
          "id": "step5",
          "type": "text",
          "content": "Geschafft! Deine Kamera zeichnet nun Timecode auf."
        }
      ]
    },
    {
      "id": "sony_a7siii_timecode",
      "cameraIds": ["sony_a7siii"],
      "title": "Timecode Setup",
      "steps": [
        {
          "id": "step1",
          "type": "text",
          "content": "Drücke die MENU Taste auf der Rückseite deiner Kamera."
        },
        {
          "id": "step2",
          "type": "imageWithText",
          "content": "Gehe zu 'Setup' → 'TC/UB' im Hauptmenü.",
          "imageUrl": "https://raw.githubusercontent.com/ichbinjona/marker-camera-tutorials/main/assets/tutorials/sony_a7siii_menu.png",
          "caption": "Das TC/UB Menü im Setup-Bereich"
        },
        {
          "id": "step3",
          "type": "text",
          "content": "Wähle 'Timecode Preset' und stelle die Startzeit mit dem Dial-Rad ein."
        },
        {
          "id": "step4",
          "type": "text",
          "content": "Setze 'TC Run' auf 'Rec Run' damit der Timecode nur während der Aufnahme läuft."
        },
        {
          "id": "step5",
          "type": "text",
          "content": "Geschafft! Der Timecode wird nun in deine Video-Dateien eingebettet."
        }
      ]
    },
    {
      "id": "canon_r5c_timecode",
      "cameraIds": ["canon_r5c"],
      "title": "Timecode Setup",
      "steps": [
        {
          "id": "step1",
          "type": "text",
          "content": "Schalte die Kamera in den Video-Modus mit dem Mode-Schalter."
        },
        {
          "id": "step2",
          "type": "imageWithText",
          "content": "Drücke MENU und navigiere zum 'Recording' Tab (roter Kamera-Symbol).",
          "imageUrl": "https://raw.githubusercontent.com/ichbinjona/marker-camera-tutorials/main/assets/tutorials/canon_r5c_menu.png",
          "caption": "Das Recording-Menü mit dem roten Kamera-Icon"
        },
        {
          "id": "step3",
          "type": "text",
          "content": "Scrolle zu 'Timecode' und wähle 'Set' um die Startzeit einzustellen."
        },
        {
          "id": "step4",
          "type": "text",
          "content": "Stelle 'Count Up' auf 'Rec Run' für Timecode nur während der Aufnahme."
        },
        {
          "id": "step5",
          "type": "text",
          "content": "Perfekt! Deine Canon R5 C zeichnet jetzt Timecode auf."
        }
      ]
    },
